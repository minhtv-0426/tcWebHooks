<?xml version="1.0" encoding="UTF-8"?>
<settings>

  <webhooks enabled="true">
    <webhook url="http://127.0.0.1:9200/teamcity-${now(yyyy-MM)}/buildevent" enabled="true" format="nvpairs">
      <states>
        <state type="buildFinished" enabled="true" />
        <state type="buildSuccessful" enabled="true" />
        <state type="buildInterrupted" enabled="false" />
        <state type="responsibilityChanged" enabled="false" />
        <state type="buildFailed" enabled="true" />
        <state type="buildFixed" enabled="false" />
        <state type="buildBroken" enabled="false" />
        <state type="beforeBuildFinish" enabled="false" />
        <state type="buildStarted" enabled="true" />
      </states>
      <build-types enabled-for-all="true" enabled-for-subprojects="true" />
    </webhook>
    <webhook url="http://127.0.0.1:9200/teamcity-${now(yyyy-MM)}/buildevent" enabled="true" format="json">
      <states>
        <state type="buildFinished" enabled="true" />
        <state type="buildSuccessful" enabled="true" />
        <state type="buildInterrupted" enabled="false" />
        <state type="responsibilityChanged" enabled="false" />
        <state type="buildFailed" enabled="true" />
        <state type="buildFixed" enabled="false" />
        <state type="buildBroken" enabled="false" />
        <state type="beforeBuildFinish" enabled="false" />
        <state type="buildStarted" enabled="true" />
      </states>
      <build-types enabled-for-all="true" enabled-for-subprojects="true" />
    </webhook>
    <webhook url="http://something" enabled="false" template="legacy-nvpairs">
      <states>
        <state type="buildAddedToQueue" enabled="false" />
        <state type="buildRemovedFromQueue" enabled="false" />
        <state type="buildStarted" enabled="false" />
        <state type="changesLoaded" enabled="false" />
        <state type="buildInterrupted" enabled="false" />
        <state type="beforeBuildFinish" enabled="false" />
        <state type="buildFinished" enabled="true" />
        <state type="buildSuccessful" enabled="true" />
        <state type="buildFailed" enabled="true" />
        <state type="buildFixed" enabled="false" />
        <state type="buildBroken" enabled="false" />
        <state type="responsibilityChanged" enabled="false" />
        <state type="buildPinned" enabled="false" />
        <state type="buildUnpinned" enabled="false" />
      </states>
      <build-types enabled-for-all="true" enabled-for-subprojects="true" />
      <auth enabled="true" type="userpass" preemptive="false">
        <auth-parameters>
          <param name="username" value="testuser" />
          <param name="password" value="testpass" />
          <param name="realm" value="testrealm" />
        </auth-parameters>
      </auth>
    </webhook>
    <webhook url="http://somewhere" enabled="false" template="flowdock">
      <states>
        <state type="buildAddedToQueue" enabled="false" />
        <state type="buildRemovedFromQueue" enabled="false" />
        <state type="buildStarted" enabled="false" />
        <state type="changesLoaded" enabled="false" />
        <state type="buildInterrupted" enabled="false" />
        <state type="beforeBuildFinish" enabled="false" />
        <state type="buildFinished" enabled="true" />
        <state type="buildSuccessful" enabled="true" />
        <state type="buildFailed" enabled="true" />
        <state type="buildFixed" enabled="false" />
        <state type="buildBroken" enabled="false" />
        <state type="responsibilityChanged" enabled="false" />
        <state type="buildPinned" enabled="false" />
        <state type="buildUnpinned" enabled="false" />
      </states>
      <build-types enabled-for-all="true" enabled-for-subprojects="true" />
    </webhook>
  </webhooks>
</settings>

